{
  "name": "dracula-with-tail",
  "version": "0.0.1",
  "description": "tailwind theme based on dracula",
  "main": "dist/dracula.with.tail.css",
  "author": "@matheusps",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "exit 0",
    "build:docs": "postcss src/tailwind.css -o docs/public/dracula.with.tail.css",
    "watch:docs": "postcss --watch src/tailwind.css -o docs/public/dracula.with.tail.css",
    "build": "postcss src/tailwind.css -o dist/dracula.with.tail.css",
    "start": "postcss --watch src/tailwind.css -o dist/dracula.with.tail.css",
    "release": "np",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "np": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^1.2.0"
  }
}
